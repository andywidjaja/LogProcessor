<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="common">
      <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" />
    </sectionGroup>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <common>
    <logging>
      <factoryAdapter type="Common.Logging.NLog.NLogLoggerFactoryAdapter, Common.Logging.NLog31">
        <arg key="configType" value="INLINE" />
      </factoryAdapter>
    </logging>
  </common>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <extensions>
      <add assembly="StackifyLib.nLog" />
    </extensions>
    <!-- 
  See http://nlog-project.org/wiki/Configuration_file 
  for information on customizing logging rules and outputs.
   -->
    <variable name="dateFormat" value="${date:format=yyyy-MM-dd HH\:mm\:ss,fff}" />
    <targets async="true">
      <target name="asyncFile" xsi:type="AsyncWrapper">
        <target name="asyncLogfile" xsi:type="File" concurrentWrites="true" fileName="C:\Log\PtLogProcessor\DEV02\Log.txt" archiveFileName="C:\LogArchive\PtLogProcessor\DEV02\Log.{#}.txt" archiveEvery="Day" maxArchiveFiles="30" layout="${dateFormat} [${machinename}] [${threadid:padcharacter= :padding=-2:fixedlength=true}] ${level:uppercase=true:padcharacter= :padding=-5:fixedlength=true} ${logger:shortName=true:padcharacter= :padding=-12:fixedlength=true}  ${message}  ${exception:format=tostring}" />
      </target>
      <target name="BloombergEngineServiceMailConfiguration" xsi:type="Mail" html="false" subject="Bloomberg Engine Service Exception [${level:uppercase=true}] [DEV02]" body="${message}" to="awidjaja@performancetrust.com;mhartmann@performancetrust.com" from="bloombergengineservice@performancetrust.com" Encoding="UTF-8" enableSsl="False" smtpAuthentication="None" smtpServer="mail.pt.int" smtpPort="25" />
      <target name="IntexEngineServiceMailConfiguration" xsi:type="Mail" html="false" subject="Intex Engine Service Exception [${level:uppercase=true}] [DEV02]" body="${message}" to="awidjaja@performancetrust.com;mhartmann@performancetrust.com" from="intexengineservice@performancetrust.com" Encoding="UTF-8" enableSsl="False" smtpAuthentication="None" smtpServer="mail.pt.int" smtpPort="25" />
      <target name="IntexRouterServiceMailConfiguration" xsi:type="Mail" html="false" subject="Intex Router Service Exception [${level:uppercase=true}] [DEV02]" body="${message}" to="awidjaja@performancetrust.com;mhartmann@performancetrust.com" from="intexrouterservice@performancetrust.com" Encoding="UTF-8" enableSsl="False" smtpAuthentication="None" smtpServer="mail.pt.int" smtpPort="25" />
      <target name="KalotayBondOasEngineServiceMailConfiguration" xsi:type="Mail" html="false" subject="Kalotay Bond Oas Engine Service Exception [${level:uppercase=true}] [DEV02]" body="${message}" to="awidjaja@performancetrust.com;mhartmann@performancetrust.com" from="kalotaybondoasengineservice@performancetrust.com" Encoding="UTF-8" enableSsl="False" smtpAuthentication="None" smtpServer="mail.pt.int" smtpPort="25" />
      <target name="KalotayMuniOasEngineServiceMailConfiguration" xsi:type="Mail" html="false" subject="Kalotay Muni Oas Engine Service Exception [${level:uppercase=true}] [DEV02]" body="${message}" to="awidjaja@performancetrust.com;mhartmann@performancetrust.com" from="kalotaymunioasengineservice@performancetrust.com" Encoding="UTF-8" enableSsl="False" smtpAuthentication="None" smtpServer="mail.pt.int" smtpPort="25" />
      <target name="MasterDataServiceMailConfiguration" xsi:type="Mail" html="false" subject="Master Data Service Exception [${level:uppercase=true}] [DEV02]" body="${message}" to="psandler@performancetrust.com;awidjaja@performancetrust.com;jklimek@performancetrust.com;mhartmann@performancetrust.com" from="masterdataservice@performancetrust.com" Encoding="UTF-8" enableSsl="False" smtpAuthentication="None" smtpServer="mail.pt.int" smtpPort="25" />
      <target name="MergentEngineServiceMailConfiguration" xsi:type="Mail" html="false" subject="Mergent Engine Service Exception [${level:uppercase=true}] [DEV02]" body="${message}" to="awidjaja@performancetrust.com;mhartmann@performancetrust.com" from="mergentengineservice@performancetrust.com" Encoding="UTF-8" enableSsl="False" smtpAuthentication="None" smtpServer="mail.pt.int" smtpPort="25" />
      <target name="PerformanceTrustEngineServiceMailConfiguration" xsi:type="Mail" html="false" subject="Performance Trust Engine Service Exception [${level:uppercase=true}] [DEV02]" body="${message}" to="awidjaja@performancetrust.com;mhartmann@performancetrust.com" from="performancetrustengineservice@performancetrust.com" Encoding="UTF-8" enableSsl="False" smtpAuthentication="None" smtpServer="mail.pt.int" smtpPort="25" />
      <target name="PrepaymentVectorServiceMailConfiguration" xsi:type="Mail" html="false" subject="Prepayment Vector Service Exception [${level:uppercase=true}] [DEV02]" body="${message}" to="awidjaja@performancetrust.com;mhartmann@performancetrust.com" from="prepaymentvectorservice@performancetrust.com" Encoding="UTF-8" enableSsl="False" smtpAuthentication="None" smtpServer="mail.pt.int" smtpPort="25" />
